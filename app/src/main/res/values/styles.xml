<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="CenturyGothic_regular_font" parent="AppTheme">
        <item name="fontFamily">@string/CenturyGothicRegular_font</item>
        <item name="fontFamilyType">@string/CenturyGothicRegular_font_family</item>
    </style>

    <style name="CenturyGothic_bold_font" parent="AppTheme">
        <item name="fontFamily">@string/CenturyGothicBold_font</item>
        <item name="fontFamilyType">@string/CenturyGothicRegular_font_family</item>
    </style>
</resources>
